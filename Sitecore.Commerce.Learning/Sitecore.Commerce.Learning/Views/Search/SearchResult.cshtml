@using Himalaya.DXP.Foundation.Search.Model


@model List<SearchResult>

@{
    Layout = null;
}

<table class="table">

    @foreach (SearchResult item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>

            <td>
                @item.DisplayName
            </td>
            <td>
                @item.FullPath
            </td>
            <td>
                @item.ProductName
            </td>
        </tr>
        
    }

</table>
</body>
</html>
